{
  "include": ["../**/*"],
  "compilerOptions": {
    "sourceMap": true,
    "strict": true,
    "noImplicitReturns": true,
    "moduleResolution": "node",
    "stripInternal": true,
    "lib": [
      "esnext",
      "dom"
    ],
    "target": "esnext",
    "outDir": "./out",
    "module": "esnext",
    "declaration": true,
    "noImplicitOverride": true
  },
}